{
    "name": "Bionic Reading",
    "description": "Using this extension you will be able to read much faster!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage", "activeTab", "scripting"],
    "action": {
        "default_popup": "./popup.html"
    }
}